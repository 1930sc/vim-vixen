{
  "manifest_version": 2,
  "name": "Vim Vixen",
  "description": "Vim Vixen",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": [ "http://*/*", "https://*/*" ],
      "js": [ "build/index.js" ]
    }
  ],
  "background": {
    "scripts": [
      "build/background.js"
    ]
  },
  "permissions": [
    "sessions",
    "tabs",
    "history"
  ],
  "web_accessible_resources": [
    "build/console.html"
  ]
}
